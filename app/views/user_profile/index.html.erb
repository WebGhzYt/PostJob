<h1>All users</h1>
<div>UserId</div>


<ul class="users">

  <% @users.each do |user| %>
    <li>
     
      <%= link_to user.email, user_profile_path(user) %>
      <% if  @current_user.admin? %>
    | <%= link_to "delete", user_profile_path(user.email) , method: :delete,
                                  data: { confirm: "r you sure?" } %>
  		<% end %>
    </li>
  <% end %>


